<% layout("layouts/boilerplate") %>

<div class="col-4 offset-4 text-center mb-5">
  <h1>All animals</h1>
  <ul class="list-group">
    <% for( animal of animals ) { %>
    <li class="list-group-item">
      <a href="/animals/<%= animal._id %> "><%= animal.name %></a>
    </li>
    <% } %>
  </ul>
</div>
