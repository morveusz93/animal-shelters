<% layout("layouts/boilerplate") %>

<div class="col-6 offset-3 text-center mb-5">
  <h1>All shelters</h1>
  <ul class="list-group">
    <% for( shelter of shelters ) { %>
    <li class="list-group-item">
      <a href="/shelters/<%= shelter._id %> "><%= shelter.name %></a>
    </li>
    <% } %>
  </ul>
</div>
